{
  "name": "logos-list",
  "type": "section",
  "styles": ["logos-list.css"],
  "scripts": ["logos-list.js"],
  "requires": ["commons", "logo", "icon", "text", "ctas"],
  "validation": {
    "required": ["sectionType"],
    "properties": {
      "sectionType": {
        "type": "string",
        "const": "logos-list"
      },
      "isDisabled": {
        "type": "boolean"
      },
      "isReverse": {
        "type": "boolean"
      },
      "hasCenteredContent": {
        "type": "boolean"
      },
      "containerFields.inContainer": {
        "type": "boolean"
      },
      "containerFields.isAnimated": {
        "type": "boolean"
      },
      "containerFields.noMargin.top": {
        "type": "boolean"
      },
      "containerFields.noMargin.bottom": {
        "type": "boolean"
      },
      "containerFields.noPadding.top": {
        "type": "boolean"
      },
      "containerFields.noPadding.bottom": {
        "type": "boolean"
      },
      "containerFields.background.isDark": {
        "type": "boolean"
      },
      "text": {
        "type": "object"
      },
      "logos.source": {
        "type": "string"
      },
      "logos.scope": {
        "type": "string",
        "enum": ["all", "selections"]
      },
      "logos.logoWidth": {
        "type": "number"
      },
      "logos.showTitle": {
        "type": "boolean"
      },
      "logos.selections": {
        "type": "array"
      },
      "ctas": {
        "type": "array"
      }
    }
  }
}
