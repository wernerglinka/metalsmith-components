{
  "name": "video",
  "type": "partial",
  "styles": ["video.css"],
  "scripts": ["video.js"],
  "requires": ["overlay", "image", "icon"],
  "validation": {
    "required": ["id", "tn"],
    "properties": {
      "id": {
        "type": "string"
      },
      "src": {
        "type": "string",
        "enum": ["youtube", "vimeo", "cloudinary"],
        "default": "youtube"
      },
      "tn": {
        "type": "string"
      },
      "alt": {
        "type": "string"
      },
      "cloudname": {
        "type": "string"
      },
      "start": {
        "type": "string"
      },
      "end": {
        "type": "string"
      },
      "inSitu": {
        "type": "boolean"
      }
    }
  }
}
